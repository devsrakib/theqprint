    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.expertsquad.theqprint"
    },
    "extra": {
      "eas": {
        "projectId": "3480cda9-e204-41ae-8ea9-6f261ea80fd6"
